{
  "name": "jani-ecosystem-monorepo",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "install:all": "npm install",
    "build:all": "npm run build --workspaces --if-present",
    "start:all": "npm-run-all --parallel start --workspaces"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
